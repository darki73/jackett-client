        "prettier": "^1.18.2",
        "rimraf": "^3.0.0",
        "ts-node": "^8.4.1",
        "ts-node-dev": "^1.0.0-pre.44",
        "tslint": "^5.20.0",
        "typescript": "^3.6.4"
    },
    "dependencies": {
        "@types/request": "^2.48.3",
        "@types/request-promise-native": "^1.0.17",
